{
    "parameters": {
        "repetitions": 1,
        "ip": "0.0.0.0",
        "port": 45001,
        "time": 30,
        "datagram-size": 1472,
        "with-gso-buffer": 64768,
        "with-mss": 1472,
        "multiplex-port": "individual",
        "multiplex-port-receiver": "individual",
        "with-gsro": false,
        "with-core-affinity": true,
        "with-numa-affinity": false,
        "with-socket-buffer": true,
        "with-ip-frag": false,
        "without-non-blocking": false,
        "parallel": 1,
        "io-model": "select",
        "interval": 0.5
    },
    "receiver same bytes per operation": {
        "recvmsg+gro": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gso-buffer": 64768,
                "with-gsro": true
            }
        },
        "recvmmsg": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 44
            }
        },
        "io-uring": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-mode": "normal",
                "uring-sq-mode": "syscall",
                "uring-burst-size": 44 
            }
        },
        "io-uring+gro": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "with-gsro": true,
                "uring-mode": "normal",
                "uring-sq-mode": "syscall",
                "uring-ring-size": 1
            }
        },
        "io-uring multishot": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-mode": "multishot",
                "uring-sq-mode": "syscall",
                "uring-burst-size": 44 
            }
        }
    }
}
