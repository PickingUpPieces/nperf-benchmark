{
    "parameters": {
        "repetitions": 3,
        "ip": "0.0.0.0",
        "port": 45001,
        "time": 10,
        "datagram-size": 1472,
        "with-gso-buffer": 65507,
        "with-mss": 1472,
        "multiplex-port": "individual",
        "multiplex-port-server": "individual",
        "with-gsro": false,
        "with-core-affinity": true,
        "with-numa-affinity": true,
        "with-socket-buffer": false,
        "with-ip-frag": false,
        "without-non-blocking": false,
        "parallel": 1,
        "io-model": "select"
    },
    "same-bytes-per-operation": {
        "sendmsg": {
            "client": {
                "exchange-function": "msg"
            },
            "server": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "sendmsg+gso": {
            "client": {
                "exchange-function": "msg",
                "with-gso-buffer": 64768,
                "with-gsro": true
            },
            "server": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "sendmmsg": {
            "client": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 44
            },
            "server": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "io-uring": {
            "client": {
                "io-model": "io-uring",
                "uring-mode": "normal",
                "uring-sq-mode": "syscall",
                "uring-burst-size": 44 
            },
            "server": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        }
    }
}
