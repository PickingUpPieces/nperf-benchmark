{
    "parameters": {
        "repetitions": 1,
        "ip": "0.0.0.0",
        "port": 45001,
        "datagram-size": 1472,
        "time": 30,
        "with-gso-buffer": 64768,
        "with-mss": 1472,
        "with-socket-buffer": false,
        "multiplex-port": "individual",
        "multiplex-port-receiver": "individual",
        "with-gsro": false,
        "with-mmsg-amount": 128,
        "with-ip-frag": false,
        "with-core-affinity": true,
        "with-numa-affinity": true,
        "without-non-blocking": false,
        "io-model": "select",
        "uring-mode": "normal",
        "uring-sqpoll": false,
        "uring-sqpoll-shared": false,
        "uring-task-work": "default",
        "uring-sq-mode": "topup",
        "uring-ring-size": 512,
        "parallel": 1,
        "interval": 0.5
    },
    "SQ-POLL-sender": {
        "io-uring": {
            "sender": {
                "io-model": "io-uring",
                "uring-ring-size": 16
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "SQ-Poll-same-NUMA": {
            "sender": {
                "io-model": "io-uring",
                "uring-ring-size": 16,
                "uring-sqpoll": true
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "SQ-Poll-other-NUMA": {
            "sender": {
                "io-model": "io-uring",
                "uring-ring-size": 16,
                "uring-sqpoll": true,
                "with-numa-affinity": false
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        }
    }
}
