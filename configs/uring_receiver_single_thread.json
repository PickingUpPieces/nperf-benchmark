{
    "parameters": {
        "repetitions": 1,
        "ip": "0.0.0.0",
        "port": 45001,
        "datagram-size": 1472,
        "time": 30,
        "with-gso-buffer": 64768,
        "with-mss": 1472,
        "with-socket-buffer": false,
        "multiplex-port": "individual",
        "multiplex-port-receiver": "individual",
        "with-gsro": false,
        "with-mmsg-amount": 128,
        "with-ip-frag": false,
        "with-core-affinity": true,
        "with-numa-affinity": true,
        "without-non-blocking": false,
        "io-model": "select",
        "uring-mode": "normal",
        "uring-sqpoll": false,
        "uring-sqpoll-shared": false,
        "uring-task-work": "default",
        "uring-sq-mode": "topup",
        "uring-ring-size": 512,
        "parallel": 1,
        "interval": 0.5
    },
    "receive-uring": {
        "recvmsg": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "exchange-function": "msg"
            }
        },
        "recvmmsg (16)": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 16
            }
        },
        "io-uring": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-ring-size": 16
            }
        },
        "io-uring provided buffer": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-mode": "provided-buffer",
                "uring-ring-size": 16
            }
        },
        "io-uring multishot": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-mode": "multishot",
                "uring-ring-size": 16
            }
        }
    }
}