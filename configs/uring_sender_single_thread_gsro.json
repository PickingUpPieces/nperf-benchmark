{
    "parameters": {
        "repetitions": 1,
        "ip": "0.0.0.0",
        "port": 45001,
        "time": 30,
        "datagram-size": 1472,
        "with-gso-buffer": 64768,
        "with-mss": 1472,
        "multiplex-port": "individual",
        "multiplex-port-receiver": "individual",
        "with-gsro": true,
        "with-core-affinity": true,
        "with-numa-affinity": false,
        "with-socket-buffer": 10,
        "with-ip-frag": false,
        "without-non-blocking": false,
        "parallel": 1,
        "io-model": "select",
        "uring-mode": "normal",
        "uring-sqpoll": false,
        "uring-sqpoll-shared": false,
        "uring-task-work": "default",
        "uring-sq-mode": "topup",
        "interval": 0.5
    },
    "send-uring": {
        "sendmsg": {
            "sender": {
                "exchange-function": "msg"
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "sendmmsg (16)": {
            "sender": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 16,
                "parallel": 1
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "io-uring": {
            "sender": {
                "io-model": "io-uring",
                "uring-ring-size": 256
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "io-uring zerocopy": {
            "sender": {
                "io-model": "io-uring",
                "uring-mode": "zerocopy",
                "uring-ring-size": 256
            },
            "receiver": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        }
    }
}
