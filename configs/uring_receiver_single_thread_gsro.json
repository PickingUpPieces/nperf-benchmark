{
    "parameters": {
        "repetitions": 1,
        "ip": "0.0.0.0",
        "port": 45001,
        "time": 30,
        "datagram-size": 1472,
        "with-gso-buffer": 64768,
        "with-mss": 1472,
        "multiplex-port": "individual",
        "multiplex-port-receiver": "individual",
        "with-gsro": true,
        "with-core-affinity": true,
        "with-numa-affinity": false,
        "with-socket-buffer": true,
        "with-ip-frag": false,
        "without-non-blocking": false,
        "io-model": "select",
        "parallel": 1,
        "interval": 0.5
    },
    "receive-uring-gsro": {
        "recvmsg": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "exchange-function": "msg"
            }
        },
        "recvmmsg (16)": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 16
            }
        },
        "io-uring": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-ring-size": 32
            }
        },
        "io-uring provided buffer": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-mode": "provided-buffer",
                "uring-ring-size": 32
            }
        },
        "io-uring multishot": {
            "sender": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "receiver": {
                "io-model": "io-uring",
                "uring-mode": "multishot",
                "uring-ring-size": 32
            }
        }
    }
}
