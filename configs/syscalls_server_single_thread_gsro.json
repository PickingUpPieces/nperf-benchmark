{
    "parameters": {
        "repetitions": 1,
        "ip": "0.0.0.0",
        "port": 45001,
        "time": 15,
        "datagram-size": 1472,
        "with-gso-buffer": 64768,
        "with-mss": 1472,
        "multiplex-port": "individual",
        "multiplex-port-server": "individual",
        "with-gsro": false,
        "with-core-affinity": true,
        "with-numa-affinity": false,
        "with-socket-buffer": false,
        "with-ip-frag": false,
        "without-non-blocking": false,
        "parallel": 1,
        "io-model": "select",
        "interval": 0.5
    },
    "single-server-gro": {
        "recvmsg": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "exchange-function": "msg"
            }
        },
        "recvmmsg": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 16
            }
        },
        "recvmsg+gro": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "exchange-function": "msg",
                "with-gsro": true
            }
        },
        "recvmmsg+gro": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 16,
                "with-gsro": true
            }
        }
    }
}
