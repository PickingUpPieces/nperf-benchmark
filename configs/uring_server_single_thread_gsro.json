{
    "parameters": {
        "repetitions": 1,
        "ip": "0.0.0.0",
        "port": 45001,
        "time": 15,
        "datagram-size": 1472,
        "with-gso-buffer": 65507,
        "with-mss": 1472,
        "multiplex-port": "individual",
        "multiplex-port-server": "individual",
        "with-gsro": true,
        "with-core-affinity": true,
        "with-numa-affinity": true,
        "with-socket-buffer": false,
        "with-ip-frag": false,
        "without-non-blocking": false,
        "io-model": "select",
        "parallel": 1,
        "interval": 0.5
    },
    "receive-uring-gsro": {
        "recvmsg": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "exchange-function": "msg"
            }
        },
        "sendmmsg-16": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "exchange-function": "mmsg",
                "with-mmsg-amount": 16
            }
        },
        "io-uring": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "io-model": "io-uring",
                "uring-ring-size": 16
            }
        },
        "io-uring provided buffer": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "io-model": "io-uring",
                "uring-mode": "provided-buffer",
                "uring-ring-size": 16
            }
        },
        "io-uring multishot": {
            "client": {
                "exchange-function": "msg",
                "with-gsro": true
            },
            "server": {
                "io-model": "io-uring",
                "uring-mode": "multishot",
                "uring-ring-size": 16
            }
        }
    }
}
